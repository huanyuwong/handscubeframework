{
    "name": "huanyuwong/handscubeframework",
    "description" : "Handscube kernel framework",
    "keywords": ["handscube"],
    "license": "MIT",
    "require": {
        "php" : ">=7.0.0",
        "illuminate/database": "^5.5",
        "symfony/http-foundation": "^3.4",
        "symfony/console": "^3.4",
        "predis/predis": "^1.1",
        "psr/http-message": "^1.0",
        "lcobucci/jwt": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable"
    },
    "autoload":{
    	"psr-4": {
            "Handscube\\" : "src/"
        }
    },
    "autoload-dev": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
